    "pretty-bytes": "^5.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "16.8.6",
    "react-lazyload": "^2.3.0",
    "react-mapbox-gl": "^3.8.0",
    "react-move": "^2.7.0",
    "react-radial": "^1.0.8",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "react-timeseries-charts": "^0.12.8",
    "recharts": "^1.0.0-beta.10",
    "redux": "4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
